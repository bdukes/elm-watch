{
  "private": true,
  "type": "module",
  "name": "example",
  "scripts": {
    "postinstall": "elm-tooling install",
    "start": "run-pty % elm-watch hot % npm run esbuild -- --serve=7000 --servedir=public % node proxy.js 7000",
    "start-advanced": "run-pty run-pty.json",
    "build": "elm-watch make --optimize && npm run esbuild -- --minify",
    "esbuild": "esbuild app.ts public/submodules/codebase-ui/src/ucm.js public/submodules/codebase-ui/src/unisonShare.js --bundle --outdir=public/build --loader:.svg=file --loader:.woff2=file"
  },
  "devDependencies": {
    "elm-tooling": "1.6.0",
    "elm-watch": "file:../build",
    "esbuild": "0.13.13",
    "run-pty": "2.3.2",
    "uglify-js": "3.14.3"
  }
}
