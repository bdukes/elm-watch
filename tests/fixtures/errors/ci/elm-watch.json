{
  "targets": {
    "admin": {
      "inputs": ["src/Admin.elm"],
      "output": "build/admin.js"
    },
    "app": {
      "inputs": ["src/App.elm"],
      "output": "build/app.js"
    },
    "postprocess-error": {
      "inputs": ["src/App.elm"],
      "postprocess": ["node", "-e", "process.exit(1)"],
      "output": "build/postprocess-error.js"
    }
  }
}